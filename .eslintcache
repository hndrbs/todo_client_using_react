tainer fluid className=\"d-flex flex-column justify-content-center h-100 text-center\">\r\n        <Container className=\"mx-auto w-75\">\r\n          {\r\n            this.state.errors.length\r\n              ? <ErrorAlert\r\n                errors={this.state.errors}\r\n                close={this.setErrors}\r\n              />\r\n              : null\r\n          }\r\n          <Form onSubmit={ this.login }>\r\n            <span className=\"h4 mx-auto\">\r\n              Welcome to Todo App <br/> Please login here\r\n            </span>\r\n            <Form.Group>\r\n              <Form.Label> Email Address </Form.Label>\r\n              <Form.Control\r\n                type=\"email\" placeholder=\"Enter your email here\" \r\n                name=\"email\"\r\n                onChange={ this.changeState }\r\n                autoComplete=\"on\"\r\n              />\r\n            </Form.Group>\r\n\r\n            <Form.Group>\r\n              <Form.Label> Password </Form.Label>\r\n              <Form.Control \r\n                type=\"password\" placeholder=\"Enter your email here\"\r\n                name=\"password\"\r\n                onChange={ this.changeState }\r\n                autoComplete=\"on\"\r\n              />\r\n            </Form.Group>\r\n            <Form.Control  \r\n              type=\"submit\" \r\n              className=\"btn btn-success\"\r\n              value=\"Login\" \r\n            />\r\n          </Form>\r\n        </Container>\r\n        <Container className=\"mt-3\">\r\n          <span className=\"h5\">Do not have an account yet?</span>\r\n          <span className=\"h5\"> create one \r\n            <a\r\n            href=\"#\"\r\n            onClick={this.toRegister}\r\n            className=\"badge badge-success mx-2\"\r\n            >here</a>\r\n          </span>\r\n        </Container>\r\n      </Container>\r\n    )\r\n  }\r\n}\r\n\r\nexport default Login\r\n","D:\\Hendri\\h8\\p3\\w0\\todo_client_using_react\\src\\views\\register-